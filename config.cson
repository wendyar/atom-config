"*":
  "atom-material-ui":
    colors:
      abaseColor: "#5c6bc0"
      paintCursor: true
      predefinedColor: "Indigo"
    tabs:
      tintedTabBar: true
    treeView:
      blendTabs: true
    ui:
      panelShadows: true
  core:
